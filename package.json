{
  "name": "gdq-viewport-assign",
  "version": "0.6.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:browserify": "node ./node_modules/browserify/bin/cmd.js src/main.ts -p [ tsify --noImplicitAny] > dist/bundle.js",
    "build:copyhtml": "cpy \"**/*.html\" ../dist/ --cwd=src --parents",
    "build:copycss": "cpy \"**/*.css\" ../dist/ --cwd=src --parents",
    "build:copyassets": "cpy \"**/*.*\" ../../dist/assets/ --cwd=src/assets --parents",
    "watch": "npm-run-all -p watch:*",
    "watch:browserify": "node ./node_modules/watchify/bin/cmd.js src/main.ts -o dist/bundle.js -p [ tsify --noImplicitAny ]",
    "watch:copyhtmlcss": "onchange -i ./src -- npm-run-all -s build:copy*"
  },
  "author": "Mark Schwartzkopf",
  "dependencies": {
    "obs-websocket-js": "^4.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "browserify": "^17.0.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "tsify": "^5.0.4",
    "typescript": "^4.5.5",
    "watchify": "^4.0.0"
  }
}
